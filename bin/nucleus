#!/usr/bin/env node

const { argv } = process;
const nucleus = require("commander");
const NucleusCLI = require("../packages/cli/dist/nucleus-cli");

nucleus.version("1.0.0", "-v, --version");
nucleus
  .command("install")
  .description("Installs a Nucleus instance on your AWS account")
  .action(async () => {
    await new NucleusCLI().install();
  });
nucleus.parse(argv);
