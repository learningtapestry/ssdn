{
  "extends": ["../../tslint.json", "tslint-react"],
  "rules": {
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "max-line-length": [
      true,
      {
        "limit": 100,
        "ignore-pattern": "https?://"
      }
    ]
  },
  "linterOptions": {
    "exclude": ["src/aws-exports.js"]
  }
}
