{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": true,
  "rules": {
    "no-console": false,
    "max-line-length": [
      true,
      100
    ],
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "**/node_modules/**"
    ]
  }
}
