{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": true,
  "rules": {
    "no-console": false,
    "max-line-length": [
      true,
      {
        "limit": 100,
        "ignore-pattern": "https?://"
      }
    ],
    "interface-name": [true, "never-prefix"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["**/node_modules/**"]
  }
}
